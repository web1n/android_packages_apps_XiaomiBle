<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="@string/earbuds_settings">

    <ListPreference
        android:defaultValue="00"
        android:entries="@array/config_equalizer_mode_entries"
        android:entryValues="@array/config_equalizer_mode_values"
        android:key="equalizer_mode"
        android:summary="%s"
        android:title="@string/equalizer_mode" />

    <ListPreference
        android:defaultValue="0000"
        android:entries="@array/config_noise_cancellation_mode_entries"
        android:entryValues="@array/config_noise_cancellation_mode_values"
        android:key="noise_cancellation_mode"
        android:summary="%s"
        android:title="@string/noise_cancellation_mode" />

    <Preference
        android:key="serial_number"
        android:selectable="false"
        android:summary="@string/device_not_connected"
        android:title="@string/serial_number" />

</PreferenceScreen>
